{
	"compilerOptions": {
		"target": "ES2020",
		"module": "nodenext",
		"strict": false,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"outDir": "dist",
		"rootDir": ".",
		"allowJs": true,
		"noImplicitAny": false
	},
	"include": [
		"*.ts",
		"**/*.ts"
	],
	"exclude": [
		"node_modules",
		"dist",
		"__out",
		"_out"
	]
}